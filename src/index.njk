---
layout: base
---

<h1>Welcome!</h1>

{% set postslist = collections.posts %}
{% for post in postslist %}
  <li>
    <a href="{{ post.url | url }}">
      {% if post.data.title %}
        {{ post.data.title }}
      {% else %}
      <code>
        {{ post.url }}
      </code>
      {% endif %}
    </a>
  </li>
{% endfor %}
